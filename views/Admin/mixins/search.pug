mixin search(keyword)
  form(id="form-search")
    .input-group.mt-2 
      input(
        type="text"
        placeholder="Nhập từ khóa"
        name="keyword"
        class="form-control"
        value=keyword
      ) 
      button(
        class="btn btn-success"
        type="submit"
        style="height: 100%;"
        ) Tìm